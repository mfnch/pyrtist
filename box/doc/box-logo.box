include "g"


ls = LineStyle[.a=.b=.c=.d=5.0]
w = Window[][
  // The X are just two lines with variable widths
  \ .Line[0.5, line.smooth, ls,
          (31, 45), 0.7, 0.4, (28, 36), 0.4, 0.5, (24, 31), 0.6, (24, 30);
          (25, 42), 0.7, 0.6, (28, 36), 0.5, 0.4, (33.5, 32),
          (34, 27), 0.4, 0.5, (32, 22)]

  // The o is a ring
  \ .Circle[(c=(28.8, 30.8)), 2.3; c+(0.1, 0.1), color.white, 1.5]
]

\ Window[][
  \ .Put[w, .Scale[(5, -5)]]
  .Save["box-logo.eps"]
]
